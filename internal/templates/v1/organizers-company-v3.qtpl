            <path fill="#4d515c" style="fill: var(--color1, #4d515c)" d="M16 7.781c-4.538 0.002-8.217 3.68-8.219 8.219v0c0.002 4.538 3.68 8.217 8.219 8.219h0c4.538-0.002 8.217-3.68 8.219-8.219v-0c-0.002-4.538-3.68-8.217-8.219-8.219h-0zM16 21.331c-2.944-0-5.331-2.387-5.331-5.332s2.387-5.332 5.332-5.332c0 0 0 0 0.001 0h-0c2.945 0 5.331 2.387 5.331 5.332s-2.387 5.332-5.332 5.332c-0 0-0 0-0.001 0h0zM26.463 7.456c0 0.015 0.001 0.033 0.001 0.051 0 1.060-0.86 1.92-1.92 1.92s-1.92-0.86-1.92-1.92c0-0.018 0-0.036 0.001-0.053l-0 0.003c0.028-1.038 0.877-1.869 1.919-1.869s1.891 0.831 1.919 1.867l0 0.003z"></path>
          </symbol>
          <symbol id="icon-reddit" viewBox="0 0 32 32">
            <path fill="#4d515c" style="fill: var(--color1, #4d515c)" d="M16 32c8.836 0 16-7.164 16-16s-7.164-16-16-16-16 7.164-16 16 7.164 16 16 16z"></path>
            <path fill="#fff" style="fill: var(--color2, #fff)" d="M26.667 16c0-1.291-1.048-2.339-2.339-2.339-0.637 0-1.199 0.243-1.609 0.653-1.591-1.14-3.8-1.889-6.232-1.983l1.067-4.996 3.461 0.729c0.037 0.892 0.768 1.601 1.665 1.601 0.92 0 1.667-0.746 1.667-1.667 0-0.003 0-0.006-0-0.009v0c0-0.917-0.749-1.667-1.667-1.667-0.653 0-1.216 0.376-1.477 0.937l-3.873-0.824c-0.025-0.005-0.055-0.008-0.085-0.008-0.087 0-0.168 0.024-0.238 0.065l0.002-0.001c-0.091 0.061-0.157 0.153-0.186 0.26l-0.001 0.003-1.179 5.576c-2.488 0.075-4.716 0.805-6.325 1.984-0.418-0.4-0.984-0.649-1.608-0.655h-0.001c-1.291 0-2.339 1.048-2.339 2.339 0 0.955 0.561 1.76 1.385 2.133-0.037 0.224-0.056 0.467-0.056 0.711 0 3.593 4.173 6.493 9.337 6.493 5.165 0 9.339-2.9 9.339-6.493 0-0.243-0.020-0.467-0.057-0.692 0.8-0.402 1.341-1.213 1.348-2.151v-0.001zM10.667 17.667c0-0.92 0.748-1.667 1.667-1.667 0.916 0 1.664 0.748 1.664 1.667 0 0.916-0.748 1.664-1.665 1.664s-1.665-0.748-1.665-1.665zM19.967 22.063c-1.141 1.143-3.312 1.217-3.948 1.217s-2.827-0.093-3.948-1.217c-0.078-0.079-0.126-0.188-0.126-0.309s0.048-0.229 0.127-0.309l-0 0c0.079-0.078 0.188-0.126 0.309-0.126s0.229 0.048 0.309 0.127l-0-0c0.711 0.712 2.245 0.973 3.349 0.973s2.62-0.263 3.349-0.973c0.079-0.079 0.189-0.127 0.309-0.127s0.23 0.049 0.309 0.127l-0-0c0.059 0.079 0.094 0.178 0.094 0.286 0 0.129-0.051 0.246-0.133 0.332l0-0zM19.668 19.331c-0.917 0-1.667-0.748-1.667-1.665s0.749-1.665 1.667-1.665 1.665 0.748 1.665 1.667c0 0.916-0.748 1.664-1.667 1.664z"></path>
          </symbol>
          <symbol id="icon-twitter_x" viewBox="0 0 32 32">
            <path fill="#4d515c" style="fill: var(--color1, #4d515c)" d="M24.435 2.539h4.499l-9.827 11.231 11.56 15.284h-9.052l-7.089-9.269-8.112 9.269h-4.501l10.511-12.013-11.089-14.501h9.281l6.408 8.472 7.413-8.472zM22.856 26.36h2.493l-16.089-21.271h-2.673l16.269 21.271z"></path>
          </symbol>
        </defs>
      </svg>

      <div
        class="js-company company-card"
        data-company-id="{%dl company.ID %}"
        data-company-name="{%s company.Name %}"
        data-company-type="{%s string(company.Type) %}"
      >
        <aside class="company-card__action">
          {% if favorite %}
            <button class="js-company-favorite favorite company-card__action-button button-group__item in-favorite" title="Remove from favorites">
              <svg xmlns="http://www.w3.org/2000/svg" xml:space="preserve" class="favorite__icon" viewBox="0 0 28 28">
                <path
                  d="m14.5 22.1-.5-.3-.5.3-6.8 4.2c-.5.3-1.1-.1-.9-.7L7.5 18l.1-.6-.4-.4-5.9-5.2c-.3-.3-.3-.6-.2-.8.1-.2.3-.4.5-.4l7.9-.7.6-.1.2-.6 2.9-7.4c.2-.5 1-.5 1.2 0l3.1 7.3.2.5.6.1 7.9.7c.2 0 .4.2.5.5.1.3 0 .6-.2.7l-5.9 5.2-.4.4.1.6 1.8 7.7c.1.3 0 .5-.2.6-.2.1-.5.2-.8 0l-6.6-4z"
                />
              </svg>
            </button>
          {% else %}
            <button class="js-company-favorite favorite company-card__action-button button-group__item" title="Add to favorite">
              <svg xmlns="http://www.w3.org/2000/svg" xml:space="preserve" class="favorite__icon" viewBox="0 0 28 28">
                <path
                  d="m14.5 22.1-.5-.3-.5.3-6.8 4.2c-.5.3-1.1-.1-.9-.7L7.5 18l.1-.6-.4-.4-5.9-5.2c-.3-.3-.3-.6-.2-.8.1-.2.3-.4.5-.4l7.9-.7.6-.1.2-.6 2.9-7.4c.2-.5 1-.5 1.2 0l3.1 7.3.2.5.6.1 7.9.7c.2 0 .4.2.5.5.1.3 0 .6-.2.7l-5.9 5.2-.4.4.1.6 1.8 7.7c.1.3 0 .5-.2.6-.2.1-.5.2-.8 0l-6.6-4z"
                />
              </svg>
            </button>
          {% endif %}
          {% comment %}
            <a href="{%s organizerFeature.Path %}/{%s company.LinkedInProfile.Alias %}" class="button-group__item" title="View statistics">
              <img width="20" height="20" alt="icon stats" src="/assets/images/pages/common/stats.svg" />
            </a>
          {% endcomment %}
        </aside>
        <figure class="company-card__header">
          <div class="company-card__logo-overlay">
            {% if company.Logo.V2 != "" %}
              <img
                class="company-card__logo"
                alt="{%s company.Name %} logo"
                src="{%s logo72x72(company.Logo) %}"
              />
            {% endif %}
          </div>
          <figcaption class="company-card__header-caption">
            <h1 class="company-card__headline vacancy__link">{%s company.Name %}</h1>
            {% comment %}
            <!-- TODO Location -->
            <p class="company-card__header-caption-text">
              <img
                class="company-card__header-caption-icon"
                alt="marker"
                width="12"
                height="12"
                src="/assets/images/pages/organizer/marker.svg"
              />Country, City (Full Remote)
            </p>
            {% endcomment %}
            {% if !company.LatestVacancyDate.IsZero() %}
            <p class="company-card__header-caption-text">
              <img
                class="company-card__header-caption-icon"
                alt="briefcase"
                width="12"
                height="12"
                src="/assets/images/pages/organizer/briefcase.svg"
              />Latest job posting: <time datetime="{%s company.LatestVacancyDate.Format(`2006-01-02`) %}">{%s company.LatestVacancyDate.Format("02 Jan 2006") %}</time>
            </p>
            {% endif %}
          </figcaption>
        </figure>
        <div class="company-card__top-links">
          <a href="{%s company.Website %}" target="_blank" class="company-card__top-link button-link">Website</a>
          {% if company.Careers == "" %}
            <span class="company-card__top-link button-link disabled">Careers</span>
          {% else %}
            <a href="{%s company.Careers %}" target="_blank" class="company-card__top-link button-link">Careers</a>
          {% endif %}
          {% if company.About == "" %}
            <span class="company-card__top-link button-link disabled">About</span>
          {% else %}
            <a href="{%s company.About %}" target="_blank" class="company-card__top-link button-link">About</a>
          {% endif %}
          {% if company.Blog == "" %}
            <span class="company-card__top-link button-link disabled">Dev Blog</span>
          {% else %}
            <a href="{%s company.Blog %}" target="_blank" class="company-card__top-link button-link">Dev Blog</a>
          {% endif %}
          {% comment %}
          {% if company.ReferralProgram == "" %}
            <span class="company-card__top-link button-link disabled">Referral program</span>
          {% else %}
            <a href="{%s company.ReferralProgram %}" target="_blank" class="company-card__top-link button-link">Referral program</a>
          {% endif %}
          {% endcomment %}
        </div>
        <div class="company-card__stacks">
          <p class="company-card__stacks-text">Other tech stacks for this company:</p>
            <ul class="company-card__stacks-list">
              {% for _, state := range companyOrganizers(company) %}
              <li class="company-card__stacks-item">
                {% if state.Organizer.Alias == organizerFeature.Organizer.Alias %}
                <a class="company-card__stacks-link company-card__stacks-link--current" href="/{%s state.Organizer.Alias %}/companies/{%s company.LinkedInProfile.Alias %}" title="{%s state.Organizer.Title %}">
                  <img
                    class="company-card__stacks-image"
                    width="20"
                    height="20"
                    src="/assets/images/pages/organizer/stacks/{%s state.Organizer.Logo %}"
                    alt="{%s state.Organizer.Title %}"
                  />
                </a>
                {% elseif state.Available %}
                <a class="company-card__stacks-link" href="/{%s state.Organizer.Alias %}/companies/{%s company.LinkedInProfile.Alias %}" title="{%s state.Organizer.Title %}">
                  <img
                    class="company-card__stacks-image"
                    width="20"
                    height="20"
                    src="/assets/images/pages/organizer/stacks/{%s state.Organizer.Logo %}"
                    alt="{%s state.Organizer.Title %}"
                  />
                </a>
                {% else %}
                <a class="company-card__stacks-link company-card__stacks-link--disabled" href="javascript:void(0);" title="{%s state.Organizer.Title %}">
                  <img
                    class="company-card__stacks-image"
                    width="20"
                    height="20"
                    src="/assets/images/pages/organizer/stacks/{%s state.Organizer.Logo %}"
                    alt="{%s state.Organizer.Title %}"
                  />
                </a>
                {% endif %}
              </li>
              {% endfor %}
            </ul>
        </div>
        <div class="company-card__info">
          <figure class="company-card__figure">
            <img
              class="company-card__icon"
              alt="card type icon"
              width="16"
              height="16"
              src="/assets/images/pages/vacancy/building.svg"
            />
            <figcaption class="company-card__figcaption">{%s companyTypeName[company.Type] %}</figcaption>
          </figure>
          <figure class="company-card__figure">
            <img
              class="company-card__icon"
              alt="card type icon"
              width="16"
              height="16"
              src="/assets/images/pages/vacancy/company-type.svg"
            />
            <figcaption class="company-card__figcaption">{%s industryNames(company.Industries) %}</figcaption>
          </figure>
          {% comment %}
          <!-- TODO Clouds -->
          <figure class="company-card__figure">
            <img
              class="company-card__icon"
              alt="card hosting icon"
              width="16"
              height="16"
              src="/assets/images/pages/organizer/clouds.svg"
            />
            <figcaption class="company-card__figcaption card__figcaption--images">
              <img
                alt="Azure icon"
                width="20"
                height="20"
                src="/assets/images/pages/common/azure.svg"
              />
              <img
                alt="AWS icon"
                width="20"
                height="20"
                src="/assets/images/pages/common/amazon-web-services.svg"
              />
              <img
                alt="GCP icon"
                width="20"
                height="20"
                src="/assets/images/pages/common/googlecloud.svg"
              />
            </figcaption>
          </figure>
          {% endcomment %}
        </div>
        <div class="company-card__info-group">
          <div class="company-card__info-group-content">
            <p class="company-card__text">{%s company.ShortDescription %}</p>
          </div>
        </div>
        <div class="company-card__social">
          <p class="company-card__social-text">Social media:</p>
          <div class="company-card__social-links">
            <a href="javascript:void(0);"
               class="company-card__social-link company-card__social-link--disabled"
            >
              <svg class="company-card__social-image">
                <use xlink:href="#icon-instagram"></use>
              </svg>
            </a>
            <a href="javascript:void(0);"
               class="company-card__social-link company-card__social-link--disabled"
            >
              <svg class="company-card__social-image">
                <use xlink:href="#icon-reddit"></use>
              </svg>
            </a>
            <a href="javascript:void(0);"
               class="company-card__social-link company-card__social-link--disabled"
            >
              <svg class="company-card__social-image">
                <use xlink:href="#icon-twitter_x"></use>
              </svg>
            </a>
            <a href="javascript:void(0);"
               class="company-card__social-link company-card__social-link--disabled"
            >
              <svg class="company-card__social-image">
                <use xlink:href="#icon-facebook"></use>
              </svg>
            </a>
          </div>
        </div>
      </div>

      <div class="company-grid">
        <div class="company-grid__item">
          <ul class="company-grid__links-group">
            <li class="company-grid__links-item company-grid__links-item--title">
              <div class="company-grid__links-item-group">
                <img
                  class="company-grid__links-icon"
                  alt="linkedin icon"
                  width="32"
                  height="32"
                  src="/assets/images/pages/organizer/linkedin.svg"
                />
                <a href="https://www.linkedin.com/company/{%s company.LinkedInProfile.Alias %}/" target="_blank" class="company-grid__links-link">LinkedIn</a>
                {% if company.LinkedInProfile.Verified %}
                <a href="https://www.linkedin.com/company/{%s company.LinkedInProfile.Alias %}/about" target="_blank" class="company-grid__links-link company-grid__links-link--verify">
                  <img
                    class="company-grid__links-icon"
                    alt="icon"
                    src="/assets/images/pages/organizer/verified-icon.png"
                  />
                </a>
                {% endif %}
              </div>
              <ul class="company-grid__links-item-info">
                <li class="company-grid__link-stats-item">{%s formatLinkedInFollowers(company.LinkedInProfile.Followers) %} followers</li>
                <li class="company-grid__link-stats-item">{%s formatLinkedInEmployees(company.LinkedInProfile.Employees) %} employees</li>
